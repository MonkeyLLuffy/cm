body {
    font-family: Arial;
    font-size:13px;
    height: 100%;
}
html{
    height: 100%;

}
*{
    margin:0px;
    padding:0px;
}
#div_left {
    float:left;
    width:20%;
    height: 100%;
    border:2px solid #28B7E7;
    background-color:#F0F9FD;
}
#div_right {
    display:block;
    float: left;
    width: 79%;
    height: 100%;
    background-color:#F0F9FD;
    border: none;
}
iframe{
    border:none;
    width:100%;
    height:600px;
    display: block;
    margin: 0 -20px;
    padding-left:20px;
}