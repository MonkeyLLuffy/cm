body {
    font-family: Arial;
    font-size:13px;
    height: 100%;
}
 /*
     border-collapse:collapse; 表示边框合并在一起。
     border-collapse:separate;表示边框之间的间距,
     间距的大小用border-spacing:px;定义大小。
 */
table {
    border:none;
    width:900px;
    border-collapse: separate;
    border-spacing:1px;
    background-color:#CCCCCC;
    word-break:break-all;
}

.div_short {
width:40px;
}
.div_middle {
width:100px;
}
.div_long {
width:120px;
}
.div_longer {
width:200px;
}
div {
}
td {
    background-color:#F0F9FD;
    text-align:center;
}
th {
    background-color:#28B7E7;
    color:#FFFFFF;
}
th, td {
    padding:2px 2px;
}
a {
    color:#0066CC;
    text-decoration:none;
}
