*{
    margin:0px;
    padding:0px;
}
/*
    a:link - 正常，未访问过的链接
    a:visited - 用户已访问过的链接
    a:hover - 当用户鼠标放在链接上时
    a:active - 链接被点击的那一刻
*/
a{
    color: #000000; 
    text-decoration: none;
}

ul {
  list-style: none;
  margin: 0;
  padding: 0;
}

img {
    border: none;
    width: 16px;
    height: 16px;
}

#menu{
  margin-top: 0px;
}

.U1 {
  border-bottom:none;
}

.L1 a:link, .L1 a:visited{
  color: #000000;
  background-color: #28B7E7;
  display: block;
  text-decoration: none;
  height: 24px;
}

.L1 a:link span, .L1 a:visited span{
 padding-left: 15px;
 height:24px;
 vertical-align:middle;
 padding:7px 0px 0px 16px;
 cursor:pointer;
 width:100%;
}

.L1 a:hover{
  color: #000000;
  font-weight:bold;
}
/* 一级菜单 active 效果 */
.L1 a:link.active span, .L1 a:hover.active span, .L1 a:active.active span, .L1 a:visited.active span{
  font-weight:bold;
}
.L2 a:link, .L2 a:visited{
  height: 24px;
  display: block;
  text-decoration: none;
  padding-left: 13px;
  background: none;
}
.L2 a:link span, .L2 a:visited span{
 vertical-align:middle;
 padding:7px 0px 0px 36px;
 cursor:pointer;
 width:100%;
}
.L2 a:hover{
  background-color:#28B7E7;
  font-weight:bold;
}
/* 二级菜单 active 效果 */
.L2 a:link.active span, .L2 a:hover.active span, .L2 a:active.active span, .L2 a:visited.active span{
  color: #000000;
  font-weight:bold;
  background: url("") no-repeat left center;
}
#menu .L1 a {
    margin-top:1.5px;
    padding:8px;
}

#menu .U1 li a{
    margin-top:auto;
    padding:10px;
    align-content:center;
}
